expression      -> literal
                 | unary 
                 | binary
                 | grouping;
BOOLEAN         -> "true" | "false";
literal         -> NUMBER | STRING | BOOLEAN | NIL;
grouping        -> "(" expression ")";
unary           -> ( "!" | "!" ) expression;
binary          -> expression operator expression;
operator        -> "==" | "!=" | "<" | "<=" | ">" | ">="
                 | "+" | "-" | "*" | "/";